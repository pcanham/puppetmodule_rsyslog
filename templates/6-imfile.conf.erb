$ModLoad imfile
$InputFileName <%= scope.lookupvar('logfilename') %>
$InputFileTag <%= scope.lookupvar('filetag') %>
$InputFileStateFile <%= scope.lookupvar('statefile') %>
$InputFileSeverity <%= scope.lookupvar('severity') %>
$InputFileFacility <%= scope.lookupvar('factility') %>
$InputRunFileMonitor

## check for new lines every 10 seconds
$InputFilePollingInterval 10
